{
  "name": "advent_of_code",
  "type": "module",
  "version": "1.0.0",
  "description": "My solutions for advent of code",
  "workspaces": [
    "2023/*"
  ],
  "scripts": {
    "build": "tsc --build --verbose",
    "test": "vitest",
    "lint": "npx prettier . --write && eslint --ext .ts .",
    "create-solution": "node ./scripts/createSolution.js",
    "2023-all": "for dir in 2023/*/; do ts-node $dir/solution.ts; done",
    "2023-1": "ts-node 2023/day1/solution.ts",
    "2023-2": "ts-node 2023/day2/solution.ts",
    "2023-3": "ts-node 2023/day3/solution.ts",
    "2023-4": "ts-node 2023/day4/solution.ts",
    "2023-5": "ts-node 2023/day5/solution.ts",
    "2023-6": "ts-node 2023/day6/solution.ts",
    "2023-7": "ts-node 2023/day7/solution.ts",
    "2023-8": "ts-node 2023/day8/solution.ts",
    "2023-9": "ts-node 2023/day9/solution.ts",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  }
}
